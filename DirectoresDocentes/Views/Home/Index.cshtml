@{
    ViewBag.Title = "Bienvenidos al sistema de información de docentes";
}
<style>
    footer {
        bottom: 0;
        position: absolute;
        width: 100%;
    }
</style>
<div class="login_form">
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <form id="formLogin" action="/" method="post">
                <fieldset>
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <figure class="container_images">
                                <img src="data:image/png;base64," alt="Login">
                            </figure>
                            <div class="row form-group">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <input type="text" name="UserName" placeholder="Usuario" class="form-control input-sm">
                                </div>
                            </div>
                            <div class="row form-group">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <input type="password" name="Password" placeholder="Contraseña" class="form-control input-sm">
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <button class="btn btn-primary btn-block" id="">Acceder <span class="fa fa-sign-in"></span></button>
                                </div>
                            </div>
                        </div>
                        <div class="panel-footer">
                            <span id="lbl_mensaje" class="text-danger">@ViewBag.Message</span>
                        </div>
                        @Html.AntiForgeryToken()
                    </div>
                </fieldset>
            </form>
        </div>
    </div>
</div>